{
  "name": "jarvis-server",
  "description": "A server to run jarvispa.info",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "coffee-middleware": "^0.2.1",
    "coffeecup": "^0.3.21",
    "connect-busboy": "0.0.1",
    "express": "^4.1.1",
    "express-subdomains": "0.0.5",
    "morgan": "^1.0.1",
    "node-persist": "0.0.2",
    "serve-static": "^1.1.0"
  },
  "scripts": {
    "start": "nohup hotcoffee server.coffee >> jarvis.log 2>&1 &",
    "stop": "pgrep node | xargs --no-run-if-empty kill"
  }
}
